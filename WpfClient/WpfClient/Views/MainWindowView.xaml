<Window x:Class="WpfClient.MainWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindowView" Height="350" Width="525">
    <Grid>
        <TextBox Text ="{Binding MessageText}"  Height="192" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBox1" VerticalAlignment="Top" Width="312" />
        <Button Command="{Binding SendCmd}" CommandParameter="{Binding Model}" Content="Send" Height="23" HorizontalAlignment="Left" Margin="12,229,0,0" Name="button1" VerticalAlignment="Top" Width="75"/>
        <StackPanel Height="192" HorizontalAlignment="Left" Margin="352,12,0,0" Name="stackPanel1" VerticalAlignment="Top" Width="139">
            <Label Content="Contacts:" Height="28" Name="label1" />
            <TextBox Height="23" Name="textBox2" Width="120" Text="Slava" IsEnabled="False" />
            <TextBox Height="23" Name="textBox3" Width="120" IsEnabled="False" Text="Igor" />
            <TextBox Height="23" Name="textBox4" Width="120" Text="Olexiy" IsEnabled="False" />
            <TextBox Height="23" Name="textBox5" Width="120" IsEnabled="False" Text="Maks" />
            <TextBox Height="23" Name="textBox6" Width="120" IsEnabled="False" Text="Oleksandr" />
        </StackPanel>
    </Grid>
</Window>
